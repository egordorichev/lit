var a = 32

print((a += 2)) // Expected: 34
print((a -= 2)) // Expected: 32
print((a *= 2)) // Expected: 64
print((a /= 4)) // Expected: 16

print((a++)) // Expected: 17
print((a--)) // Expected: 16

print((a %= 3)) // Expected: 1

class Test {}
var test = new Test()
test.a = 32

print(test.a += 32) // Expected: 64
print(test.a #= 3) // Expected: 21