var i = 0

for (;;) {
	i++

	if (i > 4) {
		break
	}
}

print(i) // Expected: 5

for (var j = 0; j < 1; j++) {
	print(j) // Expected: 0
}

i = 0

for (var z in -10 .. 10) {
	i++
}

print(i) // Expected: 21

i = 0

for (var z in 0 .. 100) {
	if (z == 0) {
		continue
	}

	i++
	break
}

print(i) // Expected: 1

var pairs = [
	[ 0, 1 ],
	[ 2, 3 ]
]

var sum = 0

for (var pair in pairs) {
	sum += pair[0] + pair[1]
}

print(sum) // Expected: 6