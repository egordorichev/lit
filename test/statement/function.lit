function test() {
	print("Yahoo")
}

test() // Expected: Yahoo
var d = test
d() // Expected: Yahoo

function outer() {
	function inner() {
		print("good")
	}

	return inner
}

var a = outer()
a() // Expected: good

function outer(arg) {
	function inner() {
		print(arg)
	}

	return inner
}

var b = outer("begget")
var c = outer("not begget")

b() // Expected: begget
c() // Expected: not begget

function z() {
	return b
}

print(z() == b) // Expected: true