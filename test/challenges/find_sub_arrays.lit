// Challenge from https://techiedelight.com/check-subarray-with-0-sum-exists-not/

var input = [ 3, 4, -7, 3, 1, 3, 1, -4, -2, -2 ]
var length = input.length
var subArrays = []

for (var i = 0; i < length; i++) {
	var array = []
	var sum = 0

	for (var j = i; j < length; j++) {
		var value = input[j]
		sum += value
		array.add(value)

		if (sum == 0) {
			subArrays.add(array)
		}
	}
}

print(subArrays.length) // Expected: 6
print(subArrays[0]) // Expected: [ 3, 4, -7 ]