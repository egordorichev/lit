// Challenge from https://techiedelight.com/check-subarray-with-0-sum-exists-not/

var input = [ 3, 4, -7, 3, 1, 3, 1, -4, -2, -2 ]
var length = input.length
var subArrays = []

for (var i = 0; i < length; i++) {
	var a = input[i]
	var array = [ a ]
	var sum = a

	for (var j = i + 1; j < length; j++) {
		var b = input[j]
		sum += b
		array.add(b)

		if (sum == 0) {
			subArrays.add(array)
		}
	}
}

print(subArrays.length) // Expected: 6
print(subArrays[0][1]) // Expected: 4