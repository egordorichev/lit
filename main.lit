class Awesome {
	constructor(a) {
		this.prop = a
	}

	test() {
		return this.prop
	}
}

class Best : Awesome {
	constructor() {
		super("good")
	}

	test() {
		this.prop = "bad"
		print(this.prop)
		return super.test()
	}

	getTest() {
		return super.test
	}
}

var b = new Best()
// b.printTest() // Expected: bad
var z = b.getTest()
print(z()) // Expected: bad
