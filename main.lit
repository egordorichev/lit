class Test {
	constructor() {
		this.base = 1
	}

	operator > (value) {
		return this.base > value
	}

	operator >= (value) {
		return this.base >= value
	}

	operator < (value) {
		return this.base < value
	}

	operator <= (value) {
		return this.base <= value
	}
}

var t = new Test()

print(t > 1) // Expected: false
print(t >= 1) // Expected: true
print(t < 1) // Expected: false
print(t <= 1) // Expected: true
