function shout(a, ...) {
	var f = new Fiber((...) => {
		var a = ...
		print(a) // Expected: [ coffee, sugar ]
	})

	f.run(...)
}

shout("tea", "coffee", "sugar")

// [ function main ][ function shout ][ tea ][ (2) [ coffee, sugar ] ][ fiber ][ fiber ][ coffee ][ sugar ]
// [ function main ][ function shout ][ tea ][ null ]