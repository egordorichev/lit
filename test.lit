class Awesome {
	constructor() {
		this.prop = "good"
	}

	test() {
		return this.prop
	}
}

class Best : Awesome {
	printTest() {
		print(this.test())
	}

	test() {
		this.prop = "bad"
		return super.test()
	}

	getTest() {
		return super.test
	}
}

var b = new Best()
b.printTest() // Expected: bad
var z = b.getTest()
z() // Expected: bad

print(b is Awesome) // Expected: true