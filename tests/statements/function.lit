function test() {
	print("Yahoo")
}

test() // Expected: Yahoo
var d = test
d() // Expected: Yahoo

function out() {
	function inner() {
		print("good")
	}

	return inner
}

var a = out()
a() // Expected: good

function outer(arg) {
	function inner() {
		print(arg)
	}

	return inner
}

var b = outer("begget")
var c = outer("not begget")

b() // Expected: begget
c() // Expected: not begget

function z() {
	return b
}

print(z() == b) // Expected: true

function call(who = "dog") {
	print($"Calling {who}")
}

call() // Expected: Calling dog
call("pizza delivery") // Expected: Calling pizza delivery

function printAll(...) {
	for var a in ... {
		print(a)
	}
}

function shout(a, ...) {
	var b = ...
	print(a) // Expected: maybe
	printAll(...) // Expected: yea
	// Expected: test

	print(b) // Expected: [ yea, test ]
}

shout("maybe", "yea", "test")

