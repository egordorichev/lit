print(1 + 3) // Expected: 4
print(1 - 3) // Expected: -2
print(2 * 3) // Expected: 6
print(9 / 3) // Expected: 3
print(10 # 3) // Expected: 3
print(4 % 2) // Expected: 0
print(3 ** 2) // Expected: 9
print(1 << 2) // Expected: 4
print(4 >> 1) // Expected: 2
print(3 ^ 4) // Expected: 7
print(~45) // Expected: -46
print(128 | 39) // Expected: 167
print(69 & 420) // Expected: 4

print(2 + 2 * 2) // Expected: 6

print(true || true) // Expected: true
print(false || true) // Expected: true
print(true || false) // Expected: true
print(false || false) // Expected: false

print(true && true) // Expected: true
print(false && true) // Expected: false
print(true && false) // Expected: false
print(false && false) // Expected: false

print(null ?? true) // Expected: true
print(10 ?? true) // Expected: 10
print(null ?? null) // Expected: null

print("test" is String) // Expected: true
print("test" is Object) // Expected: true
print("test" is Number) // Expected: false

print(32 is Number) // Expected: true
print(32 is Object) // Expected: true

print(true is Bool) // Expected: true
print(true is Object) // Expected: true

print(() => {} is Function) // Expected: true

// We have to export it or it will get optimized out
export function a() {}

print(a is Function) // Expected: true
print(a is Object) // Expected: true

class Test {

}

print(Test is Class) // Expected: true
print(Test is Object) // Expected: false

print([] is Array) // Expected: true
print([] is Object) // Expected: true